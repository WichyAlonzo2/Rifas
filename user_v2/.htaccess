RewriteEngine On
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^([^\.]+)$ $1.php [NC,L]
RewriteRule ^([^\.]+)$ $1.html [NC,L]

RewriteEngine On
RewriteCond %{SERVER_PORT} 80
RewriteRule ^(.*)$ https://cactuswings.wichyalonzo.com/$1 [R,L]

ErrorDocument 404 https://cactuswings.wichyalonzo.com/404.php
ErrorDocument 500 https://cactuswings.wichyalonzo.com/404.php